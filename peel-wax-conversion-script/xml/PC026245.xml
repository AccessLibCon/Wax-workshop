      <identifier xmlns="http://www.loc.gov/mods/v3" type="local">PC026245</identifier>
      <titleInfo xmlns="http://www.loc.gov/mods/v3" lang="english">
         <title>I VILL BE TICKLED TO SEE YOU IN EDMONTON ACH! SUCH A TIME, VAT?</title>
      </titleInfo>
      <typeOfResource xmlns="http://www.loc.gov/mods/v3">still image</typeOfResource>
      <genre xmlns="http://www.loc.gov/mods/v3" authority="gmgpc">postcards</genre>
