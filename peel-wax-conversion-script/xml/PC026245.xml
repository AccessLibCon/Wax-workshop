         <languageTerm type="text">English</languageTerm>
      </language>
      <subject xmlns="http://www.loc.gov/mods/v3">
         <hierarchicalGeographic>
            <country>Canada</country>
            <province>Alberta</province>
            <city>Edmonton</city>
         </hierarchicalGeographic>
      </subject>
      <physicalDescription xmlns="http://www.loc.gov/mods/v3">
         <extent>1 postcard: col. ; 9 x 13.5 cm</extent>
      </physicalDescription>
      <note xmlns="http://www.loc.gov/mods/v3" type="public_description">"Orange felt flag with word ""Edmonton"" is affixed to the left side of the card; right side features an illustration of a man wearing clogs and sitting on a chair"</note>
      <subject xmlns="http://www.loc.gov/mods/v3">
         <topic>Cities &amp; towns</topic>
      </subject>
      <subject xmlns="http://www.loc.gov/mods/v3">
